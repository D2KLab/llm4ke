    <!-- https://w3id.org/polifonia/ontology/core/hasSetting -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasSetting">
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/core/isSettingFor"/>
