        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/core/Address"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/AddressComponent"/>
        <rdfs:comment xml:lang="en">The relation between an address and an address component.</rdfs:comment>
        <rdfs:label xml:lang="en">has address component</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasAgent -->

