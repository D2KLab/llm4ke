        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/Agent"/>
        <rdfs:comment xml:lang="en">The relation between an entity and an agent.</rdfs:comment>
        <rdfs:label xml:lang="en">has agent</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasAgentRole -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasAgentRole">
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/core/isAgentRoleOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/AgentRole"/>
        <rdfs:comment xml:lang="en">The relation between an entity and an agent role.</rdfs:comment>
        <rdfs:label xml:lang="en">has agent role</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasAlias -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasAlias">
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/core/isAliasOf"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/Alias"/>
        <rdfs:comment xml:lang="en">The relation between an entity and an alias.</rdfs:comment>
        <rdfs:label xml:lang="en">Has alias</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasBirthPlace -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasBirthPlace">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/hasPlace"/>
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/core/isBirthPlaceOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/core/Person"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/Place"/>
        <rdfs:comment xml:lang="en">The relation between a person and her place of birth.</rdfs:comment>
        <rdfs:label xml:lang="en">has birth place</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasCity -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasCity">
        <rdfs:subPropertyOf rdf:resource="https://w3id.org/polifonia/ontology/core/hasAddressComponent"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/core/Address"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/City"/>
        <rdfs:comment xml:lang="en">The relation between an address and its city (that is an address component).</rdfs:comment>
        <rdfs:label xml:lang="en">has city</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/polifonia/ontology/core/hasCondition -->

    <owl:ObjectProperty rdf:about="https://w3id.org/polifonia/ontology/core/hasCondition">
        <owl:inverseOf rdf:resource="https://w3id.org/polifonia/ontology/core/isConditionOf"/>
        <rdfs:domain rdf:resource="https://w3id.org/polifonia/ontology/core/Explanation"/>
        <rdfs:range rdf:resource="https://w3id.org/polifonia/ontology/core/Situation"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/core/hasExplanandum"/>
            <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/core/hasSetting"/>
        </owl:propertyChainAxiom>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/core/hasExplanans"/>
            <rdf:Description rdf:about="https://w3id.org/polifonia/ontology/core/hasSetting"/>
        </owl:propertyChainAxiom>
        <rdfs:comment xml:lang="en">contextualises an explanation</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://ontologydesignpatterns.org/wiki/Submissions:ExplanationODP"/>
        <rdfs:label xml:lang="en">has condition</rdfs:label>
    </owl:ObjectProperty>
    


