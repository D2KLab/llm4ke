                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.meteck.org/files/ontologies/stuff.owl#hasSubStuff"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.meteck.org/files/ontologies/stuff.owl#Water"/>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.meteck.org/files/ontologies/stuff.owl#hasState"/>
                                        <owl:someValuesFrom rdf:resource="http://www.meteck.org/files/ontologies/stuff.owl#Liquid"/>
