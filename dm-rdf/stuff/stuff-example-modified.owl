                                    </owl:Restriction>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.meteck.org/files/ontologies/stuff.owl#hasGranuleType"/>
                                        <owl:allValuesFrom>
                                            <owl:Class>
                                                <owl:unionOf rdf:parseType="Collection">
                                                    <rdf:Description rdf:about="http://www.meteck.org/files/ontologies/stuff.owl#Atom"/>
                                                    <rdf:Description rdf:about="http://www.meteck.org/files/ontologies/stuff.owl#Ion"/>
                                                    <rdf:Description rdf:about="http://www.meteck.org/files/ontologies/stuff.owl#Molecule"/>
                                                </owl:unionOf>
                                            </owl:Class>
                                        </owl:allValuesFrom>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.meteck.org/files/ontologies/stuff.owl#hasSubStuff"/>
                        <owl:someValuesFrom>
                            <owl:Class>
                                <owl:intersectionOf rdf:parseType="Collection">
                                    <rdf:Description rdf:about="http://www.meteck.org/files/ontologies/stuff.owl#Stuff"/>
                                    <owl:Restriction>
                                        <owl:onProperty>
                                            <rdf:Description>
                                                <owl:inverseOf rdf:resource="http://www.meteck.org/files/ontologies/stuff.owl#srInheresIn"/>
                                            </rdf:Description>
                                        </owl:onProperty>
                                        <owl:someValuesFrom rdf:resource="http://www.meteck.org/files/ontologies/stuff.owl#Solvent"/>
                                    </owl:Restriction>
                                    <owl:Restriction>
                                        <owl:onProperty rdf:resource="http://www.meteck.org/files/ontologies/stuff.owl#hasGranuleType"/>
                                        <owl:allValuesFrom>
                                            <owl:Class>
                                                <owl:unionOf rdf:parseType="Collection">
                                                    <rdf:Description rdf:about="http://www.meteck.org/files/ontologies/stuff.owl#Atom"/>
                                                    <rdf:Description rdf:about="http://www.meteck.org/files/ontologies/stuff.owl#Ion"/>
                                                    <rdf:Description rdf:about="http://www.meteck.org/files/ontologies/stuff.owl#Molecule"/>
                                                </owl:unionOf>
                                            </owl:Class>
                                        </owl:allValuesFrom>
                                    </owl:Restriction>
                                </owl:intersectionOf>
                            </owl:Class>
                        </owl:someValuesFrom>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.meteck.org/files/ontologies/stuff.owl#hasNrOfPhase"/>
                        <owl:someValuesFrom>
                            <rdfs:Datatype>
                                <owl:onDatatype rdf:resource="&xsd;int"/>
                                <owl:withRestrictions rdf:parseType="Collection">
